
@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>
<a href="/student/create">Create</a>
<table class="table table-bordered">
    <tr>
        <th>Name</th>
        <th>Id</th>
        <th>Cgpa</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.Name</td>
            <td>@item.Id</td>
            <td>@item.Cgpa</td>
        </tr>
    }
</table>

